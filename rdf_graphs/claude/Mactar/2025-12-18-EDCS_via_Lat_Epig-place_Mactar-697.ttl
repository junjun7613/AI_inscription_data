        community:EDCS-23200461_community_0,
        person:EDCS-23200461_person_0,
        rel:EDCS-23200461_rel_1 ;
    epig:place <http://example.org/place/Maktar%20/%20Makthar%20/%20Mactar%20/%20Makthars%20/%20Mactaris> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "] / o[r]dine dat[o] pe/cunia sua po/suerunt" ;
    dcterms:bibliographicCitation "CIL 08, 11815" ;
    dcterms:identifier "EDCS-23200461" ;
    rdfs:comment "Fragmentary inscription. The text indicates that someone (or multiple persons, given 'posuerunt' is plural) erected something with their own money after a decree was given by the ordo (local council). The beginning of the inscription is lost. The phrase 'ordine dato' indicates official approval by the local council of decurions." .

base:EDCS-23200462 a epig:Inscription ;
    epig:mainSubject person:EDCS-23200462_person_0 ;
    epig:mentions benef:EDCS-23200462_person_0_benef_1,
        person:EDCS-23200462_person_0 ;
    epig:place <http://example.org/place/Maktar%20/%20Makthar%20/%20Mactar%20/%20Makthars%20/%20Mactaris> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "]A pollicitationi C(ai) Verri / [3]AII / [3]CIIS / [" ;
    dcterms:bibliographicCitation "CIL 08, 11816" ;
    dcterms:identifier "EDCS-23200462" ;
    rdfs:comment "This is a highly fragmentary inscription. Only the phrase 'pollicitationi C(ai) Verri' is clearly legible, referring to a pollicitatio (formal promise or pledge) by Gaius Verrus. The remaining text consists of fragmentary letter sequences that cannot be reliably reconstructed. The inscription likely recorded a public benefaction or promise made by Verrus, but the specific nature and details are lost due to the fragmentary state of the text." .

base:EDCS-23200463 a epig:Inscription ;
    epig:mainSubject person:EDCS-23200463_person_0,
        person:EDCS-23200463_person_1 ;
