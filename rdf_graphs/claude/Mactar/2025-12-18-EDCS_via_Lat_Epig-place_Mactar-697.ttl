    epig:province province:Africa%20proconsularis ;
    epig:text "Agria C(ai) l(iberta) / Pere<g=C>riana / v(ixit) a(nnos) XXXIV / h(ic) s(ita) e(st)" ;
    dcterms:bibliographicCitation "CIL 08, 11829" ;
    dcterms:identifier "EDCS-23200470" ;
    rdfs:comment "Funerary inscription for Agria Peregrina, a freedwoman of Gaius (Agrius). She lived 34 years. The text contains a correction where 'C' was originally written but should be read as 'G' in 'Peregrina'." .

base:EDCS-23200471 a epig:Inscription ;
    epig:mainSubject person:EDCS-23200471_person_0,
        person:EDCS-23200471_person_1,
