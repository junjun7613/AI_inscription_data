    epig:province province:Africa%20proconsularis ;
    epig:text "]icci Rogati f(i)l(ius?) / [3]inus / [3]AT / [" ;
    dcterms:bibliographicCitation "CIL 08, 11817" ;
    dcterms:identifier "EDCS-23200463" ;
    rdfs:comment "Highly fragmentary inscription. Only partial names preserved. The text shows '[...]icci Rogati f(i)l(ius?)' suggesting a person whose nomen ended in -iccius, with cognomen Rogatus, son of someone named Rogatus. The following lines '[3]inus' and '[3]AT' are too fragmentary to interpret with certainty. The name structure suggests local/indigenous origin with Roman naming conventions." .

base:EDCS-23200464 a epig:Inscription ;
    epig:mainSubject person:EDCS-23200464_person_0 ;
    epig:mentions community:EDCS-23200464_community_0,
        community:EDCS-23200464_community_1,
        person:EDCS-23200464_person_0 ;
