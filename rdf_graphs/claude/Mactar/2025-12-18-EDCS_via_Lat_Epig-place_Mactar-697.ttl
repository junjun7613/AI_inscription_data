    epig:province province:Africa%20proconsularis ;
    epig:text "]INOHA[3] / [3 a] solo s[ua pecunia" ;
    dcterms:bibliographicCitation "CIL 08, 00633 (p 2372) = ILTun-01, 00516" ;
    dcterms:identifier "EDCS-15300172" ;
    rdfs:comment "The inscription is highly fragmentary with only partial text preserved. The phrase 'a solo' indicates construction from the ground up, and 's[ua pecunia' suggests the work was done at the person's own expense. The name of the person and the object constructed are not preserved in the surviving text." .

base:EDCS-15300173 a epig:Inscription ;
    epig:mainSubject person:EDCS-15300173_person_0 ;
    epig:mentions person:EDCS-15300173_person_0 ;
    epig:place <http://example.org/place/Maktar%20/%20Makthar%20/%20Mactar%20/%20Makthars%20/%20Mactaris> ;
