        rel:EDCS-15400780_rel_5,
        rel:EDCS-15400780_rel_6,
        rel:EDCS-15400780_rel_7,
        rel:EDCS-15400780_rel_8 ;
    epig:place <http://example.org/place/Maktar%20/%20Makthar%20/%20Mactar%20/%20Makthars%20/%20Mactaris> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "D(is) M(anibus) s(acrum) / C(aius) Iulius Mo[n]/tanus / iun[i]/or vixit / an[n(os) 3] / C(aius) Iulius [Pr]i/mus(?) vixit / an(n)is IIII h(ic) [s(itus) e(st)] / Iulii Secu[n]/dianus et [Co]/ronat[a] / parenti[s" ;
