        rel:EDCS-15300166_rel_2,
        rel:EDCS-15300166_rel_3,
        rel:EDCS-15300166_rel_4,
        rel:EDCS-15300166_rel_5 ;
    epig:place <http://example.org/place/Maktar%20/%20Makthar%20/%20Mactar%20/%20Makthars%20/%20Mactaris> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "Q(uinto) Rupilio Q(uinti) filio / Pap(iria) Honorato / in equestres / turmas adlec/to a divo Alexan/dro flamini p(er)p(etuo) / L(ucius) Popilius Sa/turninus / patrono in/comparabili" ;
    dcterms:bibliographicCitation "CIL 08, 00627 (p 1219) = D 01315" ;
    dcterms:identifier "EDCS-15300166" ;
    rdfs:comment "This is an honorific inscription dedicated to Quintus Rupilius Honoratus by his client Lucius Popilius Saturninus. Honoratus was enrolled into the equestrian order by the emperor Severus Alexander (referred to as 'divus Alexander', indicating the inscription was made after Alexander's death and deification in 235 CE). He served as a perpetual flamen, a local priest of the imperial cult. The inscription emphasizes the patron-client relationship between Honoratus and Saturninus." .

base:EDCS-15300167 a epig:Inscription ;
    epig:mainSubject person:EDCS-15300167_person_0 ;
    epig:mentions community:EDCS-15300167_community_0,
        person:EDCS-15300167_person_0,
        rel:EDCS-15300167_rel_1 ;
    epig:place <http://example.org/place/Maktar%20/%20Makthar%20/%20Mactar%20/%20Makthars%20/%20Mactaris> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "] P(ubli) f(ilius) Honora[tus vet(eranus)] / leg(ionis) III Aug(ustae) vixit / ann(os) LXX h(ic) s(itus) e(st)" ;
    dcterms:bibliographicCitation "CIL 08, 00628" ;
    dcterms:identifier "EDCS-15300167" ;
    rdfs:comment "Funerary inscription for a veteran of the Third Augustan Legion. The praenomen and nomen are lost in the lacuna at the beginning. The filiation 'P(ubli) f(ilius)' indicates his father was named Publius. He lived 70 years and was buried at this location (h(ic) s(itus) e(st))." .

base:EDCS-15300168 a epig:Inscription ;
    epig:mainSubject person:EDCS-15300168_person_0,
        person:EDCS-15300168_person_1 ;
    epig:mentions community:EDCS-15300168_community_0,
        community:EDCS-15300168_community_1,
        person:EDCS-15300168_person_0,
        person:EDCS-15300168_person_1,
