        community:EDCS-23200445_community_0,
        person:EDCS-23200445_person_0,
        person:EDCS-23200445_person_1,
        person:EDCS-23200445_person_2,
        rel:EDCS-23200445_rel_1,
        rel:EDCS-23200445_rel_2 ;
    epig:place <http://example.org/place/Maktar%20/%20Makthar%20/%20Mactar%20/%20Makthars%20/%20Mactaris> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "[Pro salute] Imp(eratoris) Caes(aris) M(arci) Aureli Ant/onini Aug(usti) Armeniaci Parthici Medici / maximi p(atris) p(atriae) trib(unicia) pot(estate) XXIIII co(n)s(ulis) III / [imp(eratoris) V 3 therm]as(?) M[ac]taritanor[um] / restitutas a/nno Sexti Laterani proco(n)s(ulis) c(larissimi) v(iri) L(ucius) Rupilius Au[" ;
    dcterms:bibliographicCitation "CIL 08, 11799 = ILAfr 00200 = BCTH-1955/56-178 = AE 1960, 00114 = LBIRNA 00227 = AE 2018, 01913" ;
    dcterms:identifier "EDCS-23200445" ;
    rdfs:comment "This inscription records the restoration of baths belonging to the Mactaritani, dedicated for the welfare of Emperor Marcus Aurelius. The work was carried out during the proconsulship of Sextus Lateranus (c.v.) by L. Rupilius. The dating corresponds to 170-171 CE based on Marcus Aurelius' 24th tribunician power. The text is fragmentary, particularly at the end where L. Rupilius' full name and possible titles are lost." .

