    epig:place <http://example.org/place/Maktar%20/%20Makthar%20/%20Mactar%20/%20Makthars%20/%20Mactaris> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "Cocceiae / Bassulae / Numisiae / Proculae / M(arci) Munati / Popilia/ni d(ecreto) d(ecurionum) / p(ecunia) p(ublica)" ;
    dcterms:bibliographicCitation "CIL 08, 00626 (p 1219, 2372)" ;
    dcterms:identifier "EDCS-15300165" ;
    rdfs:comment "This is an honorary inscription for Cocceia Bassula Numisia Procula, erected by decree of the decurions (d.d.) with public funds (p.p.). The inscription mentions Marcus Munatius Popilianus in the genitive case, suggesting he is her father. The inscription was authorized by the local council and paid for with public money." .

base:EDCS-15300166 a epig:Inscription ;
    epig:mainSubject person:EDCS-15300166_person_0,
