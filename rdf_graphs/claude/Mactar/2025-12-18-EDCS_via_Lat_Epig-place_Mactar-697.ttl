base:EDCS-12400004 a epig:Inscription ;
    epig:mainSubject person:EDCS-12400004_person_0 ;
    epig:mentions person:EDCS-12400004_person_0 ;
    epig:place <http://example.org/place/Maktar%20/%20Makthar%20/%20Mactar%20/%20Makthars%20/%20Mactaris> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "D(is) M(anibus) s(acrum) / Cattinosu/s fidelis <v=B>i/xit in pace a/nnis XXIII [et(?)] men[sibus 3] / [3]VI[" ;
