        rel:EDCS-25600750_rel_3,
        rel:EDCS-25600750_rel_4 ;
    epig:place <http://example.org/place/Dougga%20/%20Thugga> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "Q(uinto) Caecilio Pap(iria) / [R]estitutiano / [f]il(io) Q(uinti) Caecili / Honoratiani e(gregii) v(iri) / res p(ublica) municipii Thu[gg(ensis)] / po[s(uit)] d(ecreto) d(ecurionum) p(ecunia) p(ublica)" ;
