        person:EDCS-16300016_person_2,
        person:EDCS-16300016_person_3 ;
    epig:mentions community:EDCS-16300016_community_0,
        community:EDCS-16300016_community_1,
        person:EDCS-16300016_person_0,
        person:EDCS-16300016_person_1,
