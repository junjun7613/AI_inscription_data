        person:EDCS-25601182_person_0,
        rel:EDCS-25601182_rel_1 ;
    epig:place <http://example.org/place/Dougga%20/%20Thugga> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "Genio vici regionis II / Aurelianae sacrum / Q(uintus) Mem(mius) Fel(ix) Caecilianus / v(otum) s(olvit) l(ibens) m(erito)" ;
    dcterms:bibliographicCitation "CIL 08, 26473 = Dougga 00060" ;
    dcterms:identifier "EDCS-25601182" ;
    rdfs:comment "This is a votive inscription dedicated to the Genius (protective spirit) of a vicus (village/settlement) in region II of Aurelianae. Quintus Memmius Felix Caecilianus fulfilled a vow (votum solvit libens merito - willingly and deservedly). The inscription represents a religious dedication rather than a construction or repair project." .

base:EDCS-25601183 a epig:Inscription ;
    epig:mainSubject person:EDCS-25601183_person_0 ;
