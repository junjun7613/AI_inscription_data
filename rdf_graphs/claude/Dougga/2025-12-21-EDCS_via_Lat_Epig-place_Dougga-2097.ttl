    epig:mentions person:EDCS-15300146_person_0,
        person:EDCS-15300146_person_1,
        rel:EDCS-15300146_rel_1 ;
    epig:place <http://example.org/place/Dougga%20/%20Thugga> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "D(is) M(anibus) s(acrum) / M(arcus) Porcius / L(uci) f(ilius) Extri/catus pi/us vixit / an(nos) XXV / m(enses) IIII d(ies) X / h(ic) s(itus) e(st)" ;
    dcterms:bibliographicCitation "CIL 08, 00607 = CIL 08, 11770" ;
    dcterms:identifier "EDCS-15300146" ;
    rdfs:comment "This is a funerary inscription dedicated to the Manes (spirits of the dead). Marcus Porcius Extricatus is described as 'pius' (dutiful/pious). He died at age 25 years, 4 months, and 10 days. The inscription follows the standard Roman funerary formula with D(is) M(anibus) s(acrum) at the beginning and h(ic) s(itus) e(st) at the end." .

base:EDCS-15600970 a epig:Inscription ;
