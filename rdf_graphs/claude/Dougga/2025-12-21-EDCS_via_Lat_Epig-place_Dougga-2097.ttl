    epig:place <http://example.org/place/Dougga%20/%20Thugga> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "Q(uintus) Pacuvius Saturus fl(amen) [perp(etuus) augur c(oloniae) I(uliae) K(arthaginis)] / et Nahania Victoria [eius fl(aminica) perp(etua) s(ua) p(ecunia) f(ecerunt)]" ;
