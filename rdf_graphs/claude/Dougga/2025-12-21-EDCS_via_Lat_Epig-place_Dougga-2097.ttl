    epig:datingTo 192 ;
    epig:mainSubject person:EDCS-25601193_person_0 ;
    epig:mentions person:EDCS-25601193_person_0 ;
    epig:place <http://example.org/place/Dougga%20/%20Thugga> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "Mercurio Silvio / sacrum" ;
    dcterms:bibliographicCitation "CIL 08, 26486 = ILTun-01, 01397 = AE 1906, 00014 = AE 2016, +01901 = Aounallah-2016, p 332" ;
    dcterms:identifier "EDCS-25601193" ;
    rdfs:comment "This is a votive inscription dedicated to Mercurius Silvius, a syncretic deity combining the Roman god Mercury with the Celtic/local god Silvius. The inscription is very brief, consisting only of the dedication formula 'sacrum' (sacred to). No dedicator is explicitly named in the provided text. Mercurius Silvius represents a common phenomenon in Roman provincial religion where Roman and local deities were merged. The dating to 180-192 CE corresponds to the reign of Commodus." .

base:EDCS-25601194 a epig:Inscription ;
    epig:mainSubject person:EDCS-25601194_person_0 ;
