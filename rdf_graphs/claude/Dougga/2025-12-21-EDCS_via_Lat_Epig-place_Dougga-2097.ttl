    epig:province province:Africa%20proconsularis ;
    epig:text "Pompon[i]/ae Ingen[u]/ae Q(uinti) Pacu/vi Honor[a]/ti Rufinia[n]/[i c]oniug[i] / [3]OMP[" ;
    dcterms:bibliographicCitation "CIL 08, 26614" ;
