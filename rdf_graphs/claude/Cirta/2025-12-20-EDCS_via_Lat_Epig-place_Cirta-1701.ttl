    epig:province province:Numidia ;
    epig:text "Ego Tanrocus fe/ci taerenta(!) <n=S>auta PRESILI[ // Ipopotamia / <n=P>avi<s=P>" ;
    dcterms:bibliographicCitation "CIL 08, 07157 (p 965) = ILAlg-02-01, 00821 = AE 1902, 00006" ;
    dcterms:identifier "EDCS-13200072" ;
    rdfs:comment "This inscription appears to be heavily corrupted or fragmentary. The text contains several unclear elements: 'taerenta' (possibly 'taberna' or another word), and the relationship between the persons mentioned is unclear. The notation '<n=S>' and '<n=P>' likely indicates editorial corrections (S=singular, P=plural). The '//' indicates a line break or separation. The '[' indicates incomplete text. All names appear to be local/indigenous rather than Roman." .

base:EDCS-13200073 a epig:Inscription ;
