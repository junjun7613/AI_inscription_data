    epig:province province:Numidia ;
    epig:text "Pollicitus / III Non(as) Ianua[rias] / [S]enecione et Aem[i]/liano co(n)s(ulibus) / [de]d(icavit) XIX K(alendas) Febr(uarias) / ii[s]d(em) co(n)s(ulibus)" ;
