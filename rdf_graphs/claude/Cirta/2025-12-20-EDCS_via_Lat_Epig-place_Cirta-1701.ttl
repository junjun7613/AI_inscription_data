    epig:province province:Numidia ;
    epig:text "[Sitti]a C(ai) filia / [Cal]purnia / [Extr]icata flam(inica) / [perp(etua)] ex consen/[su po]puli ob mu/[nifi]centiam e/[ius q]uod aere / [conl]ato expos/[tul]averant / [rem]issa conla/[tio]ne s(ua) p(ecunia) p(osuit) d(ecreto) d(ecurionum)" ;
