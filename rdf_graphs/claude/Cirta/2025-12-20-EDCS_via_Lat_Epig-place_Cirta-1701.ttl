    epig:province province:Numidia ;
    epig:text "D(is) M(anibus) / Geminius / Ianuarius / Heliodori fil(ius) / v(ixit) an(nos) XXX / D(is) M(anibus) / Heliodorus / v(ixit) a(nnos) CV / h(ic) s(itus) e(st) o(ssa) t(ibi) b(ene) q(uiescant)" ;
