    epig:mentions benef:EDCS-13001994_person_0_benef_1,
        person:EDCS-13001994_person_0,
        person:EDCS-13001994_person_1,
        rel:EDCS-13001994_rel_2 ;
    epig:place <http://example.org/place/Constantine%20/%20Bordj%20Saguiet%20er%20Roum%20/%20Cirta> ;
    epig:province province:Numidia ;
    epig:text "Veneri Aug(ustae) / [3]us [3]us leg(atus) Aug(usti) pr(o) pr(aetore) de[dicavit" ;
    dcterms:bibliographicCitation "CIL 08, 06964 (p 1847) = ILAlg-02-01, 00530" ;
    dcterms:identifier "EDCS-13001994" ;
    rdfs:comment "This inscription records a dedication to Venus Augusta by an imperial legate whose name is partially lost. The dating 121-123 CE corresponds to the reign of Hadrian. The legate held the position of legatus Augusti pro praetore, indicating he was governor of an imperial province. The abbreviated form 'Aug' in the inscription refers both to Venus Augusta (the Augustan Venus) and to the emperor who appointed the legate." .

base:EDCS-13001995 a epig:Inscription ;
    epig:mainSubject person:EDCS-13001995_person_0,
        person:EDCS-13001995_person_1,
        person:EDCS-13001995_person_2 ;
    epig:mentions benef:EDCS-13001995_person_0_benef_1,
        benef:EDCS-13001995_person_1_benef_1,
        community:EDCS-13001995_community_0,
        community:EDCS-13001995_community_1,
