    epig:province province:Numidia ;
    epig:text "M(arcus) Aemili/us Q(uinti) f(ilius) Fun/danus [" ;
    dcterms:bibliographicCitation "ILAlg-02-01, 00863 = AntAfr-1973-133" ;
    dcterms:identifier "EDCS-08100230" ;
    rdfs:comment "Fragmentary inscription. The text breaks off after 'Fundanus' with '[' indicating missing text. Only the name and filiation are preserved. No career, social status, or other information can be extracted from the preserved portion." .

base:EDCS-08100231 a epig:Inscription ;
