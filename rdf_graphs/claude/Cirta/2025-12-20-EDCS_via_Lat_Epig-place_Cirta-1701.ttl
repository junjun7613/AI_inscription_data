    dcterms:identifier "EDCS-12900170" ;
    rdfs:comment "Funerary inscription for Licinia Dilecta, daughter of Lucius, who lived 100 years. The formula 'h(ic) s(ita)' indicates she is buried here." .

base:EDCS-12900171 a epig:Inscription ;
    epig:datingFrom 171 ;
    epig:datingTo 230 ;
    epig:mainSubject person:EDCS-12900171_person_0 ;
    epig:mentions person:EDCS-12900171_person_0 ;
    epig:place <http://example.org/place/Constantine%20/%20Bordj%20Saguiet%20er%20Roum%20/%20Cirta> ;
    epig:province province:Numidia ;
    epig:text "D(is) M(anibus) / Licinia / Ingenua / v(ixit) a(nnos) XXXXV / h(ic) s(ita) e(st)" ;
