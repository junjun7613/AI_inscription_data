    epig:province province:Numidia ;
    epig:text "Q(uintus) Iunius Firmi/nus P(ubli) f(ilius) Arnensis aed(ilis) / IIvir quaest(or) fl(amen) p(er)p(etuus) / v(ixit) a(nnos) LXVII h(ic) s(itus) e(st)" ;
