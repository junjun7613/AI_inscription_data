        rel:EDCS-12900174_rel_1,
        rel:EDCS-12900174_rel_2 ;
    epig:place <http://example.org/place/Constantine%20/%20Bordj%20Saguiet%20er%20Roum%20/%20Cirta> ;
    epig:province province:Numidia ;
    epig:text "D(is) M(anibus) / L(ucius) Lo<ll=II>iu[s] / <L=I>(uci) fil(ius) Quir(ina) / Rusticus / [a]<l=I>umnus / v(ixit) a(nnos) XIX" ;
    dcterms:bibliographicCitation "CIL 08, 07554 = ILAlg-02-01, 01428 = AntAfr-1973-133" ;
    dcterms:identifier "EDCS-12900174" ;
    rdfs:comment "This is a funerary inscription (Dis Manibus) for Lucius Lollius Rusticus, who died at age 19. He is identified as an alumnus (foster child or ward), son of Lucius, and member of the Quirina tribe. The inscription shows typical Roman naming conventions with praenomen, nomen, cognomen, filiation, and tribal affiliation." .

base:EDCS-12900175 a epig:Inscription ;
    epig:datingFrom -30 ;
    epig:datingTo 117 ;
    epig:mainSubject person:EDCS-12900175_person_0 ;
    epig:mentions person:EDCS-12900175_person_0 ;
    epig:place <http://example.org/place/Constantine%20/%20Bordj%20Saguiet%20er%20Roum%20/%20Cirta> ;
    epig:province province:Numidia ;
    epig:text "M(arcus) Lollius / Synecdexius / v(ixit) a(nnos) L / h(ic) s(itus) e(st) / o(ssa) t(ibi) b(ene) q(uiescant)" ;
    dcterms:bibliographicCitation "CIL 08, 07555 = ILAlg-02-01, 01429 = AntAfr-1973-133" ;
    dcterms:identifier "EDCS-12900175" ;
    rdfs:comment "This is a simple funerary inscription for Marcus Lollius Synecdexius, who lived 50 years. The Greek cognomen 'Synecdexius' suggests he was likely a freedman of the Lollius family, having adopted his former owner's praenomen and nomen. The inscription includes the standard funerary formula 'hic situs est' (here he is buried) and 'ossa tibi bene quiescant' (may your bones rest well)." .

base:EDCS-12900176 a epig:Inscription ;
    epig:datingFrom 171 ;
    epig:datingTo 230 ;
    epig:mainSubject person:EDCS-12900176_person_0,
        person:EDCS-12900176_person_1 ;
    epig:mentions person:EDCS-12900176_person_0,
