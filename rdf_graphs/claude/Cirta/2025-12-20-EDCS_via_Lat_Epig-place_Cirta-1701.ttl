    epig:place <http://example.org/place/Constantine%20/%20Bordj%20Saguiet%20er%20Roum%20/%20Cirta> ;
    epig:province province:Numidia ;
    epig:text "D(is) M(anibus) s(acrum) / Praepusa / Aug(usti) n(ostri) libert(a) / coniu(n)x / rarissima / obsequens / et casta cum grandi / pietate / marita / v(ixit) a(nnos) XXXV / h(ic) s(ita) e(st) / Onesimus / maritae" ;
