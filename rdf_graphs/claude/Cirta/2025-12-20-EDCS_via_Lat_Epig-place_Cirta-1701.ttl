        rel:EDCS-12900176_rel_1,
        rel:EDCS-12900176_rel_2,
        rel:EDCS-12900176_rel_3,
        rel:EDCS-12900176_rel_4 ;
    epig:place <http://example.org/place/Constantine%20/%20Bordj%20Saguiet%20er%20Roum%20/%20Cirta> ;
    epig:province province:Numidia ;
    epig:text "H(ic) s(ita) e(st) / Lucida / Aug(usti) verna / marito ob/sequentis/simo" ;
    dcterms:bibliographicCitation "CIL 08, 07556 = ILAlg-02-01, 00787 = AntAfr-1973-133" ;
    dcterms:identifier "EDCS-12900176" ;
    rdfs:comment "Funerary inscription. Lucida was an imperial freedwoman (verna Augusti - born in the imperial household as a slave and later freed). She erected this monument for her husband, who is not named but described as obsequentissimus (most devoted/obedient). The formula H(ic) s(ita) e(st) indicates this is the burial location." .

base:EDCS-12900177 a epig:Inscription ;
    epig:datingFrom 171 ;
    epig:datingTo 230 ;
