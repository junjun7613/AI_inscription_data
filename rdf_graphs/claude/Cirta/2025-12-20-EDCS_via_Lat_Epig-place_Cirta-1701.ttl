    epig:province province:Numidia ;
    epig:text "Imp(eratore) Caes(are) P(ublio) Licinio / Valeriano Invicto / Pio Fel(ice) Aug(usto) pont(ifice) max(imo) / tr(i)b(unicia) p(otestate) p(atre) p(atriae) proc(onsule) et / Imp(eratore) Caes(are) P(ublio) Licinio / Gallieno Inv(i)cto / Pio Felice Aug(usto) pont(ifice) / max(imo) tr(i)b(unicia) p(otestate) p(atre) p(atriae)" ;
    dcterms:bibliographicCitation "CIL 08, 10300 = Hugenberg-2005, 01492" ;
    dcterms:identifier "EDCS-25900002" ;
    rdfs:comment "This inscription dates to 253 CE and mentions the two co-emperors Valerian and Gallienus. The inscription lists their imperial titles and powers. Valerian is mentioned with proconsular power, while Gallienus is not, reflecting their respective positions in the joint rule." .

base:EDCS-25900003 a epig:Inscription ;
    epig:datingFrom 293 ;
    epig:datingTo 305 ;
    epig:mainSubject person:EDCS-25900003_person_0,
