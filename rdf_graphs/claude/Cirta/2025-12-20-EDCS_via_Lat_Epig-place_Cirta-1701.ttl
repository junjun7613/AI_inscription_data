    epig:province province:Numidia ;
    epig:text "Neptuno Aug(usto) / sacr(um) / M(arcus) Licinius Ianu/arius curator e/lonic(?) anno suo / cum {i}sport(ulis) aram / dono dedit" ;
    dcterms:bibliographicCitation "CIL 08, 06956 (p 1847) = ILAlg-02-01, 00499" ;
    dcterms:identifier "EDCS-13001986" ;
    rdfs:comment "This is a votive inscription dedicated to Neptune Augustus. Marcus Licinius Ianuarius, serving as curator, donated an altar along with sportulae (distributions or gifts) during his year in office. The term 'elonic(?)' is unclear and may be a corrupted or abbreviated word. The text shows '{i}sport(ulis)' where the 'i' appears to be an error for 'sport(ulis)' (sportulae in ablative plural)." .

base:EDCS-13001987 a epig:Inscription ;
    epig:mainSubject person:EDCS-13001987_person_0 ;
