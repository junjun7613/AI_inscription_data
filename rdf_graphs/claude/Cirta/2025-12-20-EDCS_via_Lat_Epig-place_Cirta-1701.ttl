        rel:EDCS-13001993_rel_1 ;
    epig:place <http://example.org/place/Constantine%20/%20Bordj%20Saguiet%20er%20Roum%20/%20Cirta> ;
    epig:province province:Numidia ;
    epig:text "Numini Sil/vani Aug(usti) / ⟦3⟧ae sacrum / [1] Iulius Ho/noratus d(ecreto) d(ecurionum) / s(ua) p(ecunia) l(ibens) a(nimo) fecit / ex HS CXL n(ummo) / idemque de/dicavit" ;
    dcterms:bibliographicCitation "CIL 08, 06963 = ILAlg-02-01, 02046" ;
    dcterms:identifier "EDCS-13001993" ;
    rdfs:comment "This is a votive inscription dedicated to the Numen of Silvanus Augustus. Iulius Honoratus made and dedicated this sacred monument at his own expense (140 sesterces) by decree of the decurions. The text contains an erasure (⟦3⟧ae) which may indicate damnatio memoriae or correction. The praenomen of Iulius Honoratus is missing or abbreviated as [1]." .

base:EDCS-13001994 a epig:Inscription ;
    epig:datingFrom 121 ;
    epig:datingTo 123 ;
    epig:mainSubject person:EDCS-13001994_person_0,
