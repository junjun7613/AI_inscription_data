    epig:province province:Numidia ;
    epig:text "Baebia <P=F>(ubli) f(ilia) Qui(rina) / Casta sacerdos / Iun[onis] vix(it) ann[os] / LIV h(ic) s(ita) e(st) s(it) t(ibi) t(erra) l(evis)" ;
