        rel:EDCS-14200145_rel_1 ;
    epig:place <http://example.org/place/Al-Khums%20/%20Khoms%20/%20Homs%20/%20Lebdah%20/%20Lebida%20/%20Labdah%20/%20Wadi%20Zennad%20/%20Wadi%20az%20Zannad%20/%20Leptis%20Magna%20/%20Lepcis%20Magna%20/%20Neapolis> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "]S P(ubli) Lucreti Rogatiani fil(ius) / [3 monumentum] quod opere signino pater / [3 f]ecerat sibi posterisq(ue) suis / [3 restit]uit et a fundamento erexit / [3 ex] HS LXXX milib(us) n(ummum)" ;
    dcterms:bibliographicCitation "CIL 08, 00021 (p 979) = CIL 08, 10995 = IRT 00721" ;
    dcterms:identifier "EDCS-14200145" ;
    rdfs:comment "This inscription records the restoration of a family monument. The son (person 0, name partially lost) restored and rebuilt from the foundation a monument that his father, Publius Lucretius Rogatianus (person 1), had originally constructed in opus signinum (a type of waterproof concrete) for himself and his descendants. The restoration cost 80,000 sesterces. The text is fragmentary at the beginning, with the son's praenomen and possibly part of his nomen lost." .

base:EDCS-14200146 a epig:Inscription ;
    epig:mainSubject person:EDCS-14200146_person_0 ;
    epig:mentions person:EDCS-14200146_person_0 ;
    epig:place <http://example.org/place/Al-Khums%20/%20Khoms%20/%20Homs%20/%20Lebdah%20/%20Lebida%20/%20Labdah%20/%20Wadi%20Zennad%20/%20Wadi%20az%20Zannad%20/%20Leptis%20Magna%20/%20Lepcis%20Magna%20/%20Neapolis> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "Celestis sanctissima propitia [3]eamus / [" ;
    dcterms:bibliographicCitation "CIL 08, 00022 (p 921) = CIL 08, 22686 = IRT 00268" ;
    dcterms:identifier "EDCS-14200146" ;
    rdfs:comment "The inscription is fragmentary and incomplete. 'Celestis sanctissima propitia' appears to be an invocation or dedication to Celestis, a deity (likely the Carthaginian goddess Caelestis/Tanit). The text breaks off after '[3]eamus' (possibly 'eamus' = 'let us go' or part of a longer verb form). The inscription appears to be a religious dedication or prayer rather than a biographical text. No communities, careers, or relationships can be extracted from this fragmentary text." .

base:EDCS-14200147 a epig:Inscription ;
    epig:mainSubject person:EDCS-14200147_person_0,
        person:EDCS-14200147_person_1,
        person:EDCS-14200147_person_2 ;
