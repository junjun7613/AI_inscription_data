    epig:province province:Africa%20proconsularis ;
    epig:text "Dis Mani/bus Q(uinti) Cae/cili Ceria/lis Phisch/on Q(uintus) Cae/cilius Cae/cilianus / fratri pi/issimo fecit" ;
    dcterms:bibliographicCitation "IRT 00673" ;
