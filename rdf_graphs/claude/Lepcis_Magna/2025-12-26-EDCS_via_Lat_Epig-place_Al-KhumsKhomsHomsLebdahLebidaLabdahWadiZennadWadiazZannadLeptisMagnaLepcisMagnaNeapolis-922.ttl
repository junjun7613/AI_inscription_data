    epig:province province:Africa%20proconsularis ;
    epig:text "M(arcus) Iunius [Crescens] / Afr(i) [f(ilius)] / Nemes[i Aug(ustae)] // IIvir / q(uin)q(uennalis) mu/ne/rari/us // v(otum) / s(olvit)" ;
