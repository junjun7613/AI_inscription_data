    epig:province province:Africa%20proconsularis ;
    epig:text "C(aius) Cornelius Rarus Sextius Na[3] / co(n)s(ul) XV[vir] / sacris faciu[ndis 3] / proco(n)s(ul) prov[inc(iae) Africae" ;
