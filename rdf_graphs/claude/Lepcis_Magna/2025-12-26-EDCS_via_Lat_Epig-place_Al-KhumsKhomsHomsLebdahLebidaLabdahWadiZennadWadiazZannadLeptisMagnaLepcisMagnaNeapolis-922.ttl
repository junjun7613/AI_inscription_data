    epig:province province:Africa%20proconsularis ;
    epig:text "B(onae) m(emoriae) Sam/bas filius Ste/fani lo<c=K>(i) ser<v=B>a(tori)s() / vix(it) in p(ace) ann(os) VIII / m(enses) duo dies duo d(e)f(unctus) / di{a}e Lun{n}ae m/ense Martio / ind(ictionis) octabae" ;
