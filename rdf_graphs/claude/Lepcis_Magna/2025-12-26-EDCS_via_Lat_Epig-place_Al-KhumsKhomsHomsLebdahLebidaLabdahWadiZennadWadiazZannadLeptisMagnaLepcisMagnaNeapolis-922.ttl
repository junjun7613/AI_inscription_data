        rel:EDCS-06000534_rel_1 ;
    epig:place <http://example.org/place/Al-Khums%20/%20Khoms%20/%20Homs%20/%20Lebdah%20/%20Lebida%20/%20Labdah%20/%20Wadi%20Zennad%20/%20Wadi%20az%20Zannad%20/%20Leptis%20Magna%20/%20Lepcis%20Magna%20/%20Neapolis> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "C(aius) R[u]bellius Bl[andus proco(n)s(ul) 3] / M(arcus) Etrilius Lupercus le[gatus pro praetore" ;
    dcterms:bibliographicCitation "IRT 00540 = Historia-2021-356" ;
    dcterms:identifier "EDCS-06000534" ;
    rdfs:comment "This inscription mentions two Roman officials: Gaius Rubellius Blandus, who served as proconsul (governor of a senatorial province), and Marcus Etrilius Lupercus, who served as his legatus pro praetore (deputy with praetorian power). The dating of 35-36 CE places this during the reign of Tiberius. The text is fragmentary with some letters reconstructed in brackets." .

