    epig:province province:Africa%20proconsularis ;
    epig:text "[Imp(eratore) Caesa]re divi M[arci f(ilio) M(arco) Aurelio Commodo Aug(usto)] / [3]m() pon[t(ifice) max(imo) trib(unicia) pot(estate) V3] / [deo Herc]uli Genio municipii [3] / [3]qu[3] / [3] municipii [3]mi[" ;
