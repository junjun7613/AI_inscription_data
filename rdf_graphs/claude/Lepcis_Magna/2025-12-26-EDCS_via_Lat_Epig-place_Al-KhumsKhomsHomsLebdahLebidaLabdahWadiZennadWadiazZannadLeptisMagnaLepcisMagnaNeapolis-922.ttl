    epig:province province:Africa%20proconsularis ;
    epig:text "[3] Asclae ex testamento / [3]riae Sextillae matris / [3]ius Crispus c(larissimus) i(uvenis) patruo ra/[riss]imo ex dec(reto) spl(endidissimi) ord(inis) posuit" ;
