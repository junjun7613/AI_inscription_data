    epig:province province:Africa%20proconsularis ;
    epig:text "[Imp(eratori) Caes(ari) L(ucio)] Septimio / [S]evero Pio / [Per]t[i]nac[i] Aug(usto) / [Arab(ico) Adiab]en/[ico Parth(ico)] maximo / [pont(ifici) max(imo)] trib(unicia) / [pot(estate) 3] imp(eratori) XI / [co(n)s(uli) 3 p]roco(n)s(uli) / [Lep]citan[i] / [6]" ;
