        rel:EDCS-06000284_rel_1 ;
    epig:place <http://example.org/place/Al-Khums%20/%20Khoms%20/%20Homs%20/%20Lebdah%20/%20Lebida%20/%20Labdah%20/%20Wadi%20Zennad%20/%20Wadi%20az%20Zannad%20/%20Leptis%20Magna%20/%20Lepcis%20Magna%20/%20Neapolis> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "] / Genio col(oniae) Lepc(is) / Mag(nae) / Ulpius Rogatus / Pao structor / d(onum) d(edit)" ;
    dcterms:bibliographicCitation "IRT 00281" ;
    dcterms:identifier "EDCS-06000284" ;
    rdfs:comment "This is a votive inscription dedicated to the Genius (protective spirit) of the colony of Lepcis Magna by Ulpius Rogatus Pao, who identifies himself as a structor (builder/mason). The name 'Pao' appears to be a local (Punic or Libyan) element, suggesting the dedicator was a Roman citizen of local origin. The nomen 'Ulpius' may indicate citizenship granted during the reign of Trajan or Hadrian." .

base:EDCS-06000285 a epig:Inscription ;
    epig:mainSubject person:EDCS-06000285_person_0 ;
