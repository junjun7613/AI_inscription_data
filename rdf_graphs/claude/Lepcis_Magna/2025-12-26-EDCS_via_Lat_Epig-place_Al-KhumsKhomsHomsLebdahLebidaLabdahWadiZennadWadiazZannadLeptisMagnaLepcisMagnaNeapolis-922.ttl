        person:EDCS-06000553_person_0,
        person:EDCS-06000553_person_1,
        rel:EDCS-06000553_rel_1,
        rel:EDCS-06000553_rel_2,
        rel:EDCS-06000553_rel_3 ;
    epig:place <http://example.org/place/Al-Khums%20/%20Khoms%20/%20Homs%20/%20Lebdah%20/%20Lebida%20/%20Labdah%20/%20Wadi%20Zennad%20/%20Wadi%20az%20Zannad%20/%20Leptis%20Magna%20/%20Lepcis%20Magna%20/%20Neapolis> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "Dulciti // Aur(elio) Sempronio Sereno / e(quiti) R(omano) principali Alexandr(iae) / ob insigne et admirabile / studiorum ingenium / praeter dec(urionatuum) honorem / [e]t statuam apud se conloc(atam) / [e]tiam apud Lepcimag/[nenses" ;
    dcterms:bibliographicCitation "IRT 00559 = AE 2003, 01917 = AE 2007, +01692 = AE 2007, +01694" ;
    dcterms:identifier "EDCS-06000553" ;
    rdfs:comment "This is an honorific inscription from Dulcitius to Aurelius Sempronius Serenus, a Roman equestrian who served as a principalis in Alexandria. The inscription records honors bestowed upon Serenus, including the honor of decurionate and statues placed both at Dulcitius's location and among the people of Leptis Magna. The honors were granted in recognition of Serenus's exceptional and admirable scholarly achievements (ob insigne et admirabile studiorum ingenium)." .

