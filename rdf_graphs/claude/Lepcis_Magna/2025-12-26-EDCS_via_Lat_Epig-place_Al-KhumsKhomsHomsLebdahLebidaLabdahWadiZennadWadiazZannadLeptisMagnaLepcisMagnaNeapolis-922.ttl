        rel:EDCS-06000302_rel_2 ;
    epig:place <http://example.org/place/Al-Khums%20/%20Khoms%20/%20Homs%20/%20Lebdah%20/%20Lebida%20/%20Labdah%20/%20Wadi%20Zennad%20/%20Wadi%20az%20Zannad%20/%20Leptis%20Magna%20/%20Lepcis%20Magna%20/%20Neapolis> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "Marti Augusto sacrum / auspiciis Imp(eratoris) Caesaris Aug(usti) / pontificis max<i=U>mi patris / patriae ductu Cossi Lentuli / co(n)s(ulis) XVviri sacris faciundis / proco(n)s(ulis) provincia Africa / bello Gaetulico liberata / civitas Lepcitana" ;
    dcterms:bibliographicCitation "IRT 00301 = AE 1940, 00068 = AE 1941, +00105 = AfrIt-1940/41-14 = AE 1948, +00006 = AE 1954, 00201a = AfrRom-1987, p 01-66 = AE 2000, +01592 = AE 2006, +00096 = AntAfr-2022-128,11" ;
    dcterms:identifier "EDCS-06000302" ;
    rdfs:comment "This is a dedication to Mars Augustus, erected under the auspices of Emperor Augustus and the leadership of Cossus Lentulus, consul and proconsul of Africa. The inscription commemorates the liberation of the province of Africa from the Gaetulian War. The city of Lepcis (Leptis Magna) erected this dedication. The dating of 8 CE corresponds to the period after the Gaetulian revolt was suppressed." .

