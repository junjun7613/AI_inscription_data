        rel:EDCS-06000280_rel_3,
        rel:EDCS-06000280_rel_4 ;
    epig:place <http://example.org/place/Al-Khums%20/%20Khoms%20/%20Homs%20/%20Lebdah%20/%20Lebida%20/%20Labdah%20/%20Wadi%20Zennad%20/%20Wadi%20az%20Zannad%20/%20Leptis%20Magna%20/%20Lepcis%20Magna%20/%20Neapolis> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "Fortu(n)ae / Iuliae Q(uinti) f(iliae) / Iustae / Iustus / pater / ex / d(ecreto) o(rdinis)" ;
    dcterms:bibliographicCitation "IRT 00276" ;
    dcterms:identifier "EDCS-06000280" ;
    rdfs:comment "This is a votive dedication to the goddess Fortuna on behalf of Iulia Iusta, daughter of Quintus. The dedication was made by Iustus, who identifies himself as her father (pater), by decree of the local council (ex decreto ordinis). The relationship between Quintus (person_id 1) and Iustus (person_id 2) is unclear from the text - Quintus may be Iulia's biological father while Iustus could be a stepfather or adoptive father, or there may be a textual issue. The inscription follows the common formula of dedicating to a deity for the well-being or honor of a family member." .

