        rel:EDCS-06000595_rel_5,
        rel:EDCS-06000595_rel_6 ;
    epig:place <http://example.org/place/Al-Khums%20/%20Khoms%20/%20Homs%20/%20Lebdah%20/%20Lebida%20/%20Labdah%20/%20Wadi%20Zennad%20/%20Wadi%20az%20Zannad%20/%20Leptis%20Magna%20/%20Lepcis%20Magna%20/%20Neapolis> ;
    epig:province province:Africa%20proconsularis ;
    epig:text "Ti(berio) Iulio Papir(ia) / Frontino / flam(ini) divi Aug(usti) / univer[s]us ordo / col(oniae) Lepcis Magn(ae) / merenti statuam / decrevit / Ti(berius) Iulius Ti(beri) f(ilius) Fronto / honore contentus indulgen/tissimo [p]atri de suo posuit" ;
    dcterms:bibliographicCitation "IRT 00598 = AE 2018, +01864" ;
    dcterms:identifier "EDCS-06000595" ;
    rdfs:comment "This is an honorific inscription from Lepcis Magna. The entire ordo (municipal council) decreed a statue for Tiberius Iulius Frontinus, who served as flamen of the deified Augustus. His son, Tiberius Iulius Fronto, erected the statue at his own expense (de suo), being content with the honor itself (honore contentus). The phrase 'honore contentus' suggests that Fronto declined the financial burden that would normally accompany such an honor, choosing instead to pay for the statue himself. Frontinus belonged to the Papiria tribe, one of the Roman voting tribes." .

